{
  "name": "dwnload/wp-login-locker",
  "description": "Disable direct access to your sites /wp-login.php script, plus user notifications based on actions.",
  "type": "wordpress-plugin",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Austin Passy",
      "email": "thefrosty@users.noreply.github.com",
      "homepage": "https://austin.passy.co",
      "role": "Developer"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "thefrosty/wp-utilities": "^1.1.3",
    "symfony/http-foundation": "^3.2",
    "php": ">=7.0.4",
    "pimple/pimple": "~3.0"
  },
  "require-dev": {
    "10up/wp_mock": "dev-dev",
    "phpunit/phpunit": "6.*",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "phpcs": "./vendor/bin/phpcs --standard=PSR2 --extensions=php src",
    "test": "./vendor/bin/phpunit --colors",
    "export-coverage": "test-reporter"
  },
  "autoload": {
    "psr-4": {
      "Dwnload\\WpLoginLocker\\": "src"
    },
    "files": ["helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Dwnload\\WpLoginLocker\\Tests\\": "tests/unit"
    }
  }
}
